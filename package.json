    "bin": "javascript/terminal.mjs",
    "repository": {
        "type": "git",
        "url": "https://github.com/kevwhuang/academy.git"
    },
    "scripts": {
        "autoprefixer": "postcss css/*.css -u autoprefixer -d build --nomap",
        "build": "tsc --watch",
        "deploy": "gcloud app deploy",
        "eslint": "eslint '**/*.{js,cjs,mjs,jsx,ts,tsx}'",
        "eslinthtml": "eslint **/*.html",
        "htmllint": "html-validate **/*.{html,htm,hbs}",
        "jsdoc": "jsdoc -c documents/jsdoc.json",
        "nodemon": "nodemon javascript/exp.js",
        "postdeploy": "gcloud app deploy",
        "prestart": "ts-node -v",
        "sass": "sass -w --no-source-map css:compiled",
        "start": "NODE_ENV=production ts-node server/express.mjs",
        "stylelint": "stylelint **/*.{css,sass,html}",
        "test": "jest --watchAll --config test/jest.config.json"
    },
    "config": {
        "port": 65535
    },
