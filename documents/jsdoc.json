{
    "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
    "recurseDepth": 20,
    "source": {
        "include": [
            "javascript"
        ],
        "exclude": [],
        "includePattern": "(.js$|.mjs$|.ts$)",
        "excludePattern": "(node_modules/|docs)"
    },
    "sourcetype": "module",
    "opts": {
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    }
}