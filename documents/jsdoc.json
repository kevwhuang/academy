{
    "recurseDepth": 100,
    "sourcetype": "module",
    "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
    "opts": {
        "recurse": true,
        "destination": "./docs",
        "encoding": "utf8",
        "readme": "./README.md"
    },
    "source": {
        "excludePattern": "(node_modules/|docs)",
        "includePattern": "(.js$|.mjs$|.ts$)",
        "exclude": [],
        "include": [
            "javascript"
        ]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    }
}