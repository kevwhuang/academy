{
    "name": "react-w3s",
    "version": "1.0.0",
    "author": "Kevin Huang",
    "scripts": {
        "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
        "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
        "sass": "sass --watch src/css/scss:compiled",
        "server": "json-server --watch src/resources/db.json -p 3001",
        "start": "DISABLE_ESLINT_PLUGIN=true react-scripts start"
    },
    "dependencies": {
        "@emotion/react": "*",
        "@emotion/styled": "*",
        "@mui/material": "*",
        "@mui/icons-material": "*",
        "cookie": "*",
        "json-server": "*",
        "react": "*",
        "react-dom": "*",
        "react-hook-form": "*",
        "react-icons": "*",
        "react-router-dom": "*",
        "react-scripts": "*",
        "styled-components": "5.x",
        "uuid": "*"
    },
    "devDependencies": {
        "@types/cookie": "*",
        "@types/node": "*",
        "@types/react": "*",
        "@types/react-dom": "*",
        "@types/react-router-dom": "*",
        "@types/uuid": "*",
        "eslint": "*",
        "eslint-plugin-jsx-a11y": "*",
        "eslint-plugin-react": "*",
        "eslint-plugin-react-hooks": "*",
        "sass": "*",
        "typescript": "*"
    },
    "browserslist": {
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ],
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ]
    }
}